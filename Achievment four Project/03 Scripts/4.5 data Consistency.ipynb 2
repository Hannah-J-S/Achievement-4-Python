    "#changing order_id and user_id to string\n",
    "df_orders_wrangled['user_id'] = df_orders_wrangled['user_id'].astype('str')\n",
    "df_orders_wrangled['order_id'] = df_orders_wrangled['order_id'].astype('str')"
   ]
